}\r\n\r\n/* ###########  REACT Divs */\r\n\r\n#main_content_root{\r\n    width: 100%;\r\n    height: 95%;\r\n}\r\n.screen{\r\n    background-image: url(\"images/bg_main.png\");\r\n    background-repeat: no-repeat;\r\n    background-size: 100% 100%;\r\n    width: 100%;\r\n    height: 100%;\r\n    background-color: black;\r\n    margin-top: 1.5vmin;\r\n}\r\n.main_nav{\r\n    width: 100%;\r\n    height: 100%;\r\n    display: none;\r\n}\r\n.main_nav.active{\r\n    display: grid;\r\n    grid-template-columns: 60vmin 1vmin;\r\n    grid-template-rows: auto;\r\n}\r\n\r\n.nav_widget p{\r\n    font-size: 2vmin;\r\n}\r\n.side_divider{\r\n    display: flex;  \r\n    flex-direction: column;\r\n    color: white;\r\n    align-items: center;\r\n}\r\n.intro_div{\r\n    background-color: red;\r\n}\r\n.side_divider ul{\r\n    margin-top: 10vmin;\r\n    height: 28%;\r\n    list-style: none;\r\n    font-size: 3.5vmin;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-around;\r\n    text-align: center;\r\n}\r\n.side_divider ul {\r\n    text-shadow: 1px 10px 15px rgba(0, 0, 0, 0.822);\r\n}\r\n.btn.active{\r\n    color: var(--varAccent);\r\n}\r\n.btn:hover{\r\n    color: var(--varAccent);\r\n}\r\n.btn.active::before{\r\n    content: \"|\";\r\n    width: 5vmin;\r\n    height: 100%;\r\n    background-color: var(--varAccent);\r\n}\r\n\r\n/* Navigation Widgets */\r\n/* ## Any new widgets needed to be created, make sure it has both an nth-child reference and it's :hover reference */\r\n/* Thus you may need to create 2 SVGs to create the design stuff */\r\n/*  I know this sucks but this is becuase each background Logo doesn't have a consistent size, and needs personalization. */\r\n\r\n.nav_widget{ /* Primary Design*/\r\n    color: white;\r\n    background-color: var(--varAccent3);\r\n    width: 40vmin;\r\n    border-radius: 10px;\r\n    font-size: 2.5vmin;\r\n    font-weight: 500;\r\n    height: 30vmin;\r\n    padding: 3.5vmin;\r\n    margin: 4vmin;\r\n    box-sizing: border-box;\r\n    box-shadow: 1px 10px 15px rgba(0, 0, 0, 0.438);\r\n    background-repeat: no-repeat;\r\n    transition: 0.4s ease-in-out;\r\n}\r\n.nav_widget:hover{ /* Universal Hover Style */\r\n    background-color: #E2D7D7; \r\n    color: var(--varAccent3);\r\n}\r\n\r\n\r\n\r\n.nav_widget:nth-child(1){\r\n    background-image: url(\"images/logos/UoMCal1.png\");\r\n    background-size: 50% 65%;\r\n    background-position: 20vmin 12.5vmin;\r\n\r\n}\r\n.nav_widget:nth-child(1):hover{\r\n    background-image: url(\"images/logos/UoMCal.png\");\r\n    background-position: 20vmin 11.5vmin;\r\n}\r\n/* ^ This is an example  */\r\n.nav_widget:nth-child(2){\r\n    background-image: url(\"images/logos/UoMStudent1.png\");\r\n    background-size: 35% 45%;\r\n    background-position: 22vmin 15.5vmin;\r\n\r\n}\r\n.nav_widget:nth-child(2):hover{\r\n    background-image: url(\"images/logos/UoMStudent2.png\");\r\n    background-position: 22vmin 13.5vmin;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"7d29ad6f2e2c5d611066a40d1b8d957b","cacheData":{"env":{}}}